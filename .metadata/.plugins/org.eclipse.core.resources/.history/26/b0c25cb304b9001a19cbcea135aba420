package cn.fakejson.fakejson;

import java.util.ArrayList;
import java.util.List;

import cn.fakejson.fakejson.util.JsonUtil;

public class Fakejson {

	@SuppressWarnings("unchecked")
	public static <E> List<E> fakejsons(FakeClass<E> fake) throws IllegalArgumentException, IllegalAccessException {
		List<E> list = new ArrayList<E>();

		for (int i = 0; i < fake.getSize(); i++) {
			E fakeNew = fake.createNew();

			fake.setSeq(i);
			list.add(((FakeClass<E>) fakeNew).getEntity());
		}

		System.out.println(JsonUtil.toString(list));

		return list;
	}
}

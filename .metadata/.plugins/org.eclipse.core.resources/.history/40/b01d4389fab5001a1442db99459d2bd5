package cn.fakejson.fakejson;

import java.util.ArrayList;
import java.util.List;

import cn.fakejson.fakejson.util.JsonUtil;

public class Fakejson {

	public static <E> void testfakejson(FakeClass<E> fake) {
		List<E> list = new ArrayList<E>();
		
		System.out.println(JsonUtil.toString(fake.getEntity()));

		System.out.println(JsonUtil.toString(fake.getSize()));

		System.out.println(JsonUtil.toString(list.size()));

		for (int i = 0; i < fake.getSize(); i++) {
			list.add(fake.getEntity());
		}
		
		System.out.println(JsonUtil.toString(list));
	}
}

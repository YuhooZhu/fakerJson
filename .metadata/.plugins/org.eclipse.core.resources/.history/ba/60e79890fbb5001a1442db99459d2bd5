package cn.fakejson.fakejson;

import cn.fakejson.fakejson.util.JsonUtil;

public class FakeClass<E>{

	private E entity;

	private int size;

	public E getEntity() {
		System.out.println(JsonUtil.toString(entity.getClass().getFields()));
		return entity;
	}

	public void setEntity(E entity) {
		this.entity = entity;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}
}
